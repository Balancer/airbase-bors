with(document){
<?
    ob_start();
    include("log.phtml");
    $file = split("\n",ob_get_contents());
    ob_end_clean();

    for($i=0;$i<sizeof($file);$i++)
    {
        $s=str_replace("\\","\\\\",$file[$i]);
        $s=str_replace("\"","\\\"",$s);
        $s=str_replace("\n"," ",$s);
        $s=str_replace("\r"," ",$s);
        print "write(\"$s\");\n";
    }
?>
}